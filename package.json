{
  "name": "svelte-compare-image",
  "version": "1.0.0-1",
  "author": "Brian Mitchell",
  "license": "MIT",
  "description": "A Svelte component to compare two images",
  "keywords": [
    "svelte",
    "compare",
    "image",
    "slider"
  ],
  "homepage": "https://github.com/BrianMitchL/svelte-compare-image",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrianMitchL/svelte-compare-image.git"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prettier-staged": "pretty-quick --staged",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "^1.0.0-next.222",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^27.4.0",
    "@types/micromatch": "^4.0.2",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "eslint-plugin-testing-library": "^5.0.3",
    "jest": "^27.4.7",
    "micromatch": "^4.0.4",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "simple-git-hooks": "^2.7.0",
    "svelte": "^3.45.0",
    "svelte-check": "^2.2.12",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.4.13",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "type": "module",
  "simple-git-hooks": {
    "pre-commit": "npm run prettier-staged"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
