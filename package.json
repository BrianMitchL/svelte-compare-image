{
  "name": "svelte-compare-image",
  "version": "1.0.1",
  "author": "Brian Mitchell",
  "license": "MIT",
  "description": "A Svelte component to compare two images",
  "keywords": [
    "svelte",
    "compare",
    "image",
    "slider"
  ],
  "homepage": "https://github.com/BrianMitchL/svelte-compare-image",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrianMitchL/svelte-compare-image.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "package": "svelte-package",
    "prettier-staged": "pretty-quick --staged"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.48",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "simple-git-hooks": "^2.8.1",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "simple-git-hooks": {
    "pre-commit": "npm run prettier-staged"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "pluginSearchDirs": [
      "."
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  }
}
